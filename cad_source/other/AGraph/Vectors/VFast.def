    {$IFDEF FLOAT} Float80 {$ELSE} UInt32 {$ENDIF}; override;
  {$ENDIF}
  function DotProduct(V: TGenericNumberVector):
    {$IFDEF FLOAT} Float80 {$ELSE}
      {$IFDEF INT64_VECT} Int64 {$ELSE}
        {$IFDEF UINT} UInt32 {$ELSE} Int32 {$ENDIF} {$ENDIF} {$ENDIF}; override;
